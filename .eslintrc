{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "rules": {
    "semi": [2, "never"],
    "import/prefer-default-export": false,
    "react/jsx-filename-extension": false
  },
  "plugins": [
    "flowtype"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/resolver": "webpack"
  }
}